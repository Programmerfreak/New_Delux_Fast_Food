<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.newdeluxfastfood">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".screens.place_new_order_screen.Menu"></activity>
        <activity android:name=".screens.place_new_order_screen.PlaceOrder" />
        <activity android:name=".screens.registration_screen.RegistrationScreen" />
        <activity android:name=".screens.login_screen.LoginScreen" />
        <activity android:name=".screens.dashboard.Dashboard" />
        <activity android:name=".screens.launch_screen.LaunchScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>